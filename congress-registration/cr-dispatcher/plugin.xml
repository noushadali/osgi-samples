<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.equinox.http.registry.resources">
		<resource
		 alias="/xsd/1.0.0"
		 base-name="/www/xsd"/>
   </extension>
   
   <extension
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/wsdlgen"
            class="com.osgisamples.congress.wsdlgenerator.WsdlServlet"/>
   </extension>
   
   <extension
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/services"
            class="org.apache.axis.transport.http.AxisServlet"/>
   </extension>
</plugin>
